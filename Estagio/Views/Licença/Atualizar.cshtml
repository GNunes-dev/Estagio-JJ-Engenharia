@{
    ViewData["Title"] = "Gerar Licença";
    Layout = "~/Views/Shared/_LayoutADM.cshtml";
}

@section js{
    <script src="~/view/Licença/atualizar.js"></script>
}

<div class="col-md-8">
    <div class="card">
        <div class="card-header card-header-primary">
            <h4 class="card-title">Gerar Licença</h4>
        </div>
        <div class="card-body">

            <form>
              
                <div class="form-row">
                    <div class="form-group col-md-3">
                        <label for="versao">Versão</label>
                        <input type="text" class="form-control" id="versao">
                    </div>

                    <div class="form-group col-md-3">
                        <label for="vencimento">Data de Vencimento</label>
                        <input type="date" class="form-control" id="vencimento">
                    </div>

                </div>
                <br />
             
                <div id="divMsg"></div>
                <div class="text-center">
                    <input type="button" class="btn btn-primary" value="ATUALIZAR" id="btnAtualizar" onclick="atualizar.btnAtualizar()" />
                </div>
            </form>
        </div>
    </div>
</div>