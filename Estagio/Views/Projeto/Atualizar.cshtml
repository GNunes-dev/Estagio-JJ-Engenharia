@{
    ViewData["Title"] = "Projeto";
    Layout = "~/Views/Shared/_LayoutADM.cshtml";
}

@section js{
    <script src="~/view/Projeto/atualizar.js"></script>
}

<div class="col-md-8">
    <div class="card">
        <div class="card-header card-header-primary">
            <h4 class="card-title">Projeto</h4>
        </div>
        <div class="card-body">

            <form>
              
                <div class="form-row">
                    <div class="form-group col-md-8">
                        <label for="status">Status</label>
                        <textarea class="form-control" id="status" rows="5"></textarea>
                    </div>
                </div>
                <br />
             
                <div id="divMsg"></div>
                <div class="text-center">
                    <input type="button" class="btn btn-primary" value="ATUALIZAR" id="btnAtualizar" onclick="atualizar.btnAtualizar()" />
                </div>
            </form>
        </div>
    </div>
</div>